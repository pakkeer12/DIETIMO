  <section id="BF">
    <div class="container " style="margin-top:110px;padding:20px;">
      <h1 style="text-align:center;">BREAKFAST</h1>


      <div class="owl-carousel owl-theme">
      <% for(var i = 0; i<breakfast.length; i++){ %>

        <div class="item">
          <div class="card " style="width: 15rem; height: 31rem;">
            <img class="card-img-top owl-lazy" data-src= <%= breakfast[i].image  %> alt="Card image cap">
            <div class="card-body">
              <h5 class="card-title"><%= breakfast[i].title  %></h5>
              <p class="card-text">Calories - <%= breakfast[i].calories  %>cal</p>
              <p class="card-text">Protien - <%= breakfast[i].protein  %></p>
              <p class="card-text">Fat - <%= breakfast[i].fat  %></p>
              <p class="card-text">Carbohydrates - <%= breakfast[i].carbs  %></p>
              <form action="/output" method="post">
                  <button class="btn btn-primary" name="know" type="submit" value= <%= breakfast[i].id %> >Know more </button>
              </form>
            </div>
          </div>
        </div>
      <% } %>

      </div>
    </div>
  </section>
